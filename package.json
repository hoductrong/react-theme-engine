    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "@rollup/plugin-node-resolve": "^15.2.2",
    "@rollup/plugin-commonjs": "^25.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^5.2.2",
    "rollup": "^3.24.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
