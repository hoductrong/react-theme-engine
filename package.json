{
  "name": "react-theme-engine",
  "version": "0.3.0-preview.e85ef5c",
  "description": "Simple react theme provider for react applications",
  "main": "./dist/bundle.js",
  "module": "./dist/bundle.esm.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:hoductrong/react-theme-engine.git",
  "author": "Trong Ho <trong.hoduc@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "build": "yarn clean && rollup -c",
