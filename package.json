{
  "name": "react-theme-engine",
  "version": "1.0.1",
  "description": "Simple react theme provider for react applications",
  "main": "./dist/bundle.js",
  "module": "./dist/bundle.esm.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/hoductrong/react-theme-engine",
  "author": "Trong Ho <trong.hoduc@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "react",
    "theme",
    "provider",
    "engine",
    "theme-engine",
    "react-theme-engine",
    "hooks",
    "css-variable",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "private": false,
  "scripts": {
    "build": "yarn clean && rollup -c",
    "lint": "next lint",
    "clean": "rimraf dist",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "test": "jest --passWithNoTests",
    "prepare-manifest:preview": "./scripts/prepare-preview-manifest.sh",
    "publish-preview": "yarn publish --tag preview"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-node-resolve": "^15.2.2",
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.24.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
